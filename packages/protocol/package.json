{
  "name": "@mcpambassador/protocol",
  "version": "0.8.0-beta.1",
  "description": "MCP Ambassador Protocol - Type-only package defining API contracts between client and server",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
